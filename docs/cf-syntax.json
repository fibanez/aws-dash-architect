    "AWS::ResilienceHub::ResiliencyPolicy",
    "AWS::Chatbot::SlackChannelConfiguration",
    "Alexa::ASK::Skill",
    "Custom::*"
  ],
  "punctuation": {
    "pairs": [
      ["[", "]"],
      ["{", "}"],
      ["(", ")"]
    ],
    "special": [":", ",", "-", ".", "=", ">", "<", "&", "|", "!", "$", "*", "/", "+", "?"]
  },
  "string_delimiters": ["\"", "'", "|", ">"],
  "numeric_patterns": [
    "^-?\\d+$",
    "^-?\\d+\\.\\d+$",
    "^-?\\d+\\.\\d+[eE][+-]?\\d+$",
    "^0x[0-9a-fA-F]+$"
  ],
  "token_rules": {
    "resource_type_pattern": "^(AWS|Alexa|Custom)::[A-Za-z0-9]+::[A-Za-z0-9]+$",
    "intrinsic_function_pattern": "^(Fn::|!)\\w+$",
    "pseudo_parameter_pattern": "^AWS::[A-Za-z]+$",
    "parameter_type_pattern": "^(String|Number|List<.+>|CommaDelimitedList|AWS::.+)$"
  },
  "highlighting_priority": [
    "comments",
    "strings",
    "functions",
    "aws_resource_types",
    "types",
    "keywords",
    "special",
    "numeric",
    "punctuation"
  ],
  "case_sensitive": true,
  "multiline_strings": {
    "yaml_literal": {
      "start": "|",
      "indent_based": true
    },
    "yaml_folded": {
      "start": ">",
      "indent_based": true
    }
  },
  "additional_patterns": {
    "template_version": "2010-09-09",
    "common_attributes": [
      "AuthenticationConfiguration",
      "SkillPackage",
      "VendorId",
      "ResourceSignal",
      "AutoScalingReplacingUpdate",
      "AutoScalingRollingUpdate",
      "AutoScalingScheduledAction",
      "AutoScalingCreationPolicy"
    ],
    "deletion_policies": [
      "Delete",
      "Retain",
      "Snapshot",
      "RetainExceptOnCreate"
    ]
  }
}
